<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
	  <ServiceWorkerAssetsManifest>service-worker-assets.js</ServiceWorkerAssetsManifest>
    <BlazorCacheBootResources>false</BlazorCacheBootResources>
    <BlazorEnableTimeZoneSupport>false</BlazorEnableTimeZoneSupport>
    <BlazorWebAssemblyPreserveCollationData>false</BlazorWebAssemblyPreserveCollationData>
    <UserSecretsId>cb0171fc-fc45-4dcf-b056-a4c027094415</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="5.0.1" />
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="5.0.1" PrivateAssets="all" />
    <PackageReference Include="Syncfusion.Blazor.Buttons" Version="18.4.0.35" />
    <PackageReference Include="Syncfusion.Blazor.Calendars" Version="18.4.0.35" />
    <PackageReference Include="Syncfusion.Blazor.Charts" Version="18.4.0.35" />
    <PackageReference Include="Syncfusion.Blazor.DropDowns" Version="18.4.0.35" />
    <PackageReference Include="Syncfusion.Blazor.Grid" Version="18.4.0.35" />
    <PackageReference Include="Syncfusion.Blazor.Inputs" Version="18.4.0.35" />
    <PackageReference Include="Syncfusion.Blazor.Navigations" Version="18.4.0.35" />
    <PackageReference Include="Syncfusion.Blazor.Popups" Version="18.4.0.35" />
    <PackageReference Include="System.Net.Http.Json" Version="5.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ServiceWorker Include="wwwroot\service-worker.js" PublishedContent="wwwroot\service-worker.published.js" />
  </ItemGroup>
</Project>
